{
  "name": "api",
  "version": "1.0.0",
  "dependencies": {
    "@prisma/client": "^4.1.1",
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.17.2",
    "helmet": "^5.1.0",
    "i18next": "^21.8.9",
    "i18next-http-middleware": "^3.2.1",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.7.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/pg": "^8.6.4",
    "nodemon": "^2.0.15",
    "prisma": "^4.1.1",
    "ts-node": "^10.5.0",
    "typescript": "^4.5.5"
  },
  "scripts": {
    "build": "tsc",
    "start": "nodemon ./src/index.ts localhost 3001",
    "prisma:generate": "prisma generate --schema ./src/database/schema.prisma",
    "prisma:init": "prisma migrate dev --schema ./src/database/schema.prisma --name init",
    "prisma:migrate": "prisma migrate dev --schema ./src/database/schema.prisma",
    "postinstall": "npm run prisma:generate"
  }
